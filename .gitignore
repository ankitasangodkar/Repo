# ignore everything in the root except the "wp-content" directory.
/*
!.gitignore
wp-content/
wp-config.php

# ignore everything in the "wp-content" directory, except:
#plugins, and themes directories

wp-content/*
!wp-content/plugins/
!wp-content/themes/

# Ignore everything in the "plugins" directory, except the 
# plugins specified

wp-content/plugins/*
!wp-content/themes/elementor/

# Ignore everything in the "plugins" directory, except the 
# themes specified

wp-content/themes/*
!wp-content/themes/sydney/
!wp-content/themes/sydney-child/

# ignore all files starting with . or ~
.*
~*

# ignore Editor files
*.sublime-project
*.sublime-workspace
*.komodoproject

# ignore log files and databases
*.log
*.sql


# ignore compiled files
*.com
*.class
*.dll
*.exe
*.o
*.so

# ignore packaged files
*.rar
*.tar
*.zip



